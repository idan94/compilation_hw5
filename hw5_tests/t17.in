void main() {
  byte x = 200b;
  printi(x); //200
  byte y = 100b;
  printi(y); //100
  byte c = x+y;
  printi(c); // 200+100 = 300.TRUNC : 300-256 = 44
  int newx = x;
  int d = newx+y; 
  printi(d); //200 (intADD) 100 = 300
  
  
  int d2 = x+y;
  printi(d2); //200 (byteADD) 100 = 300.TRUNC : 300-256 = 44
}